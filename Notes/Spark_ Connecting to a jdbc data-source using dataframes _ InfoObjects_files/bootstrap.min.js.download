<script type="text/javascript">
/* SPOTTED TRIFORCE ARTS
 * this code will float the contact/signup button panels of 
 * tall .training_box'es automatically based on scroll values
 */
 
function floatPanels() {
    $('.training_box').each(function() {
        if ($(this).height() > 400) {
            //figure out positioning
            var box_top = $(this).position().top + 65;
            var box_height = $(this).height();
            var fold = $(window).scrollTop() + $(window).height();
            var onscreen = ((box_top + 100) < fold);
            var completed = ((box_top + box_height) < fold);
    
            //apply styles
            if (onscreen && !completed) {
                $(this).find('.track_box_panel').addClass('floatme').css('left',($(this).position().left+2-$(window).scrollLeft())+'px');
            } else {
                $(this).find('.track_box_panel').removeClass('floatme').css('left','0');
            }
        }
    });
}
$(window).scroll(floatPanels);
$(window).resize(floatPanels);
$(function() {
    floatPanels();
});
/* END SPOTTED TRIFORCE ARTS */
</script>

