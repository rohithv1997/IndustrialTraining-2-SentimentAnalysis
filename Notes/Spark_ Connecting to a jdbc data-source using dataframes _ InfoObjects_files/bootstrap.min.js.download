<script type="text/javascript">
/* SPOTTED TRIFORCE ARTS
 * this function shuffles the staff grid on the our-team page 
 */

function rearrangeProfiles() {
    if ($('.jpr_team').length != 0) {
        var deck = [];
	var t= setTimeout(function() {
            $('.team_member').each(function(index) {
                deck[index] = $(this)[0].outerHTML;
            });
            shuffle(deck);
            $('#team_box').html(deck.join(''));
        },2000);
    } 
}
    
function shuffle(array) {
    for (var i = array.length - 1; i > 0; i--) {
        var j = Math.floor(Math.random() * (i + 1));
        var temp = array[i];
        array[i] = array[j];
        array[j] = temp;
    }
    return array;
}
